
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model School {
  id      Int      @id @default(autoincrement())
  name    String
  courses Course[]
}

model Course {
  id        Int       @id @default(autoincrement())
  name      String
  schoolId  Int
  school    School    @relation(fields: [schoolId], references: [id])
  students  Student[]
}

model Student {
  id       Int    @id @default(autoincrement())
  name     String
  courseId Int
  course   Course @relation(fields: [courseId], references: [id])
}
